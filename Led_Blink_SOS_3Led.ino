*/
int led1 = 12;   //led1 pin 12
int led2 = 11;   //led2 pin 11
int led3 = 10;   //led3 pin 10

// the setup routine runs once when you press reset:
void setup() {
 
  // make the led's pin an ouput:
  pinMode(led1, OUTPUT);
  pinMode(led2, OUTPUT);
  pinMode(led3, OUTPUT);  
}
